{
  "name": "aminkt/yii2-shop-module",
  "type": "yii2-extension",
  "description": "Yii2 shop module for shopping sites",
  "keywords": ["yii2", "order", "ordering", "shop", "module"],
  "license": "All right reserved",
  "homepage": "https://gitlab.com/aminkt/yii2-shop-module",
  "support": {
    "issues": "https://gitlab.com/aminkt/yii2-shop-module/issues",
    "site": "https://telbit.ir",
    "source": "https://gitlab.com/aminkt/yii2-shop-module/issues"
  },
  "authors": [
    {
      "name": "Amin Keshavarz",
      "email": "ak_1596@yahoo.com"
    }
  ],
  "repositories": [
    {
      "type": "gitlab",
      "url": "https://gitlab.com/aminkt/yii2-ordering-module"
    }
